            "PlayerController": [
                "class PlayerController extends MonoBehaviour {",
                "",
                "  @SerializeField",
                "  playerNumber;",
                "",
                "  moveSpeed = 5;",
                "",
                "  up;",
                "  down;",
                "  right;",
                "  left;",
                "  bomb;",
                "",
                "  playerLooking;",
                "",
                "  canMove = true;",
                "",
                "  //Start is called before the first frame update",
                "  Start() {",
                "    if(this.playerNumber == 1){",
                "      this.up = KeyCode.W;",
                "      this.down = KeyCode.S;",
                "      this.right = KeyCode.D;",
                "      this.left = KeyCode.A;",
                "      this.bomb = KeyCode.Space;",
                "    }else{",
                "      this.up = KeyCode.UpArrow;",
                "      this.down = KeyCode.DownArrow;",
                "      this.right = KeyCode.RightArrow;",
                "      this.left = KeyCode.LeftArrow;",
                "      this.bomb = KeyCode.Keypad0;",
                "    }",
                "",
                "",
                "    this.playerLooking = new Vector3(0,0,1);",
                "  }",
                "  ",
                "  //Update is called once per frame",
                "  Update() {",
                "",
                "    // Movement",
                "    var newLooking = this.playerLooking;",
                "    ",
                "    if(Input.GetKey(this.right)){",
                "      newLooking = new Vector3(1,0,0);",
                "      this.Move(newLooking);",
                "    }",
                "    if(Input.GetKey(this.left)){",
                "      newLooking = new Vector3(-1,0,0);",
                "      this.Move(newLooking);",
                "    }",
                "    if(Input.GetKey(this.up)){",
                "      newLooking = new Vector3(0,0,1);",
                "      this.Move(newLooking);",
                "    }",
                "    if(Input.GetKey(this.down)){",
                "      newLooking = new Vector3(0,0,-1);",
                "      this.Move(newLooking);",
                "    }",
                "",
                "    // Bombs",
                "    if(Input.GetKeyDown(this.bomb)){",
                "      var bomb = Instantiate(\"Bomb\");",
                "      bomb.transform.position = this.transform.position;",
                "    }",
                "",
                "    // if(pos != this.transform.position){",
                "    //   this.transform.position = pos;",
                "    // }",
                "  }",
                "",
                "  Move(look){",
                "    this.playerLooking = look;",
                "",
                "    var pos = this.transform.position;",
                "    var face = this.transform.parent.Find(\"Face\");",
                "    ",
                "    face.transform.position = new Vector3(pos.x + (this.playerLooking.x  * 0.7),",
                "                                          pos.y, ",
                "                                          pos.z + (this.playerLooking.z  * 0.7));",
                "",
                "    if(this.canMove){",
                "      this.transform.position = new Vector3(pos.x + (this.playerLooking.x * this.moveSpeed * Time.deltaTime),",
                "                                            pos.y,",
                "                                            pos.z + (this.playerLooking.z * this.moveSpeed * Time.deltaTime));",
                "    }",
                "  }",
                "",
                "  ",
                "}",
                ""
            ],
            "MapGenerator": [
                "class MapGenerator extends MonoBehaviour {",
                "",
                "  mapMatrix;",
                "  width;",
                "  height;",
                "",
                "  playerArea = 3;",
                "",
                "  //Start is called before the first frame update",
                "  Start() {",
                "",
                "    var playerNumber = 1;",
                "",
                "    this.height = Math.floor(this.transform.localScale.z);",
                "    this.width = Math.floor(this.transform.localScale.x);",
                "    for(var i = -1 ; i <= this.width ; i++){",
                "      for(var j = -1 ; j <= this.height ; j++){",
                "        ",
                "        var p1Pos = (i == 0 && j == 0);",
                "        var p2Pos = (i == this.width-1 && j == this.height-1);",
                "",
                "        if(!p1Pos && !p2Pos){",
                "          var p1Area = (i < this.playerArea && j < this.playerArea)",
                "          var p2Area = (i > this.width-1 - this.playerArea && j > this.height-1 - this.playerArea)",
                "",
                "          var walls = i == -1 || j == -1 || i == this.width || j == this.height;",
                "          if(walls){",
                "            this.AddWall(i,j);",
                "          }else if (!p1Area && !p2Area && Random.Range(0,3,true) != 0){",
                "            this.AddBlock(i,j);",
                "          }",
                "        }else{",
                "          this.AddPlayer(i,j,playerNumber);",
                "          playerNumber++;",
                "        }",
                "      }",
                "    }",
                "  }",
                "  ",
                "  //Update is called once per frame",
                "  Update() {",
                "  ",
                "  }",
                "",
                "  AddBlock(i,j){",
                "    var block = Instantiate(\"Block\");",
                "    this.PlaceOnTopOfFloor(i,j,block);",
                "  }",
                "",
                "  AddWall(i,j){",
                "    var wall = Instantiate(\"Wall\");",
                "    this.PlaceOnTopOfFloor(i,j,wall);",
                "  }",
                "",
                "  AddPlayer(i,j,pNum){",
                "    var name = (pNum == 1)? \"PlayerOne\" : \"PlayerTwo\";",
                "    var player = Instantiate(name);",
                "    player = this.PlaceOnTopOfFloor(i,j,player);",
                "  }",
                "",
                "  PlaceOnTopOfFloor(i,j,obj){",
                "    var pos = this.transform.position;",
                "    var xx = pos.x - (this.width/2) + i + (obj.transform.localScale.x/2);",
                "    var yy = pos.y + (obj.transform.localScale.y/2) + (this.transform.localScale.y/2);",
                "    var zz =  pos.z - (this.height/2) + j + (obj.transform.localScale.z/2);",
                "    obj.transform.position = new Vector3(xx, yy, zz);",
                "",
                "    return obj;",
                "  }",
                "}",
                ""
            ],
            "FaceCollider": [
                "class FaceCollider extends MonoBehaviour {",
                "",
                "  //Start is called before the first frame update",
                "  Start() {",
                "  ",
                "  }",
                "  ",
                "  //Update is called once per frame",
                "  Update() {",
                "  ",
                "  }",
                "",
                "  OnTriggerStay(other : Collider){",
                "    if(other.transform.parent.name == \"Block (Clone)\" || ",
                "       other.transform.parent.name == \"Wall (Clone)\"){",
                "      this.transform.parent.Find(\"Box\").GetComponent(PlayerController).canMove = false;",
                "    }",
                "  }",
                "",
                "  OnTriggerExit(other : Collider){",
                "    if(other.transform.parent.name == \"Block (Clone)\" || ",
                "       other.transform.parent.name == \"Wall (Clone)\"){",
                "      this.transform.parent.Find(\"Box\").GetComponent(PlayerController).canMove = true;",
                "    }",
                "  }",
                "}",
                ""
