            "PlayerController": [
                "class PlayerController extends MonoBehaviour {",
                "",
                "  @SerializeField",
                "  playerNumber;",
                "",
                "  moveSpeed = 5;",
                "",
                "  //Start is called before the first frame update",
                "  Start() {",
                "  ",
                "  }",
                "  ",
                "  //Update is called once per frame",
                "  Update() {",
                "    var pos = this.transform.position;",
                "    if(Input.GetKey(KeyCode.A)){",
                "      pos = new Vector3(pos.x + (this.moveSpeed * Time.deltaTime), pos.y, pos.z);",
                "    }",
                "    if(Input.GetKey(KeyCode.D)){",
                "      pos = new Vector3(pos.x - (this.moveSpeed * Time.deltaTime), pos.y, pos.z);",
                "    }",
                "    if(Input.GetKey(KeyCode.W)){",
                "      pos = new Vector3(pos.x, pos.y, pos.z - (this.moveSpeed * Time.deltaTime));",
                "    }",
                "    if(Input.GetKey(KeyCode.S)){",
                "      pos = new Vector3(pos.x, pos.y, pos.z + (this.moveSpeed * Time.deltaTime));",
                "    }",
                "",
                "    if(pos != this.transform.position){",
                "      this.transform.position = pos;",
                "    }",
                "  }",
                "}",
                ""
            ],
            "MapGenerator": [
                "class MapGenerator extends MonoBehaviour {",
                "",
                "  mapMatrix;",
                "  width;",
                "  height;",
                "",
                "  //Start is called before the first frame update",
                "  Start() {",
                "",
                "    var playerNumber = 1;",
                "",
                "    this.height = Math.floor(this.transform.localScale.z);",
                "    this.width = Math.floor(this.transform.localScale.x);",
                "    for(var i = 0 ; i < this.width ; i++){",
                "      for(var j = 0 ; j < this.height ; j++){",
                "        if((i != 0 || j != 0) && (i != this.width-1 || j != this.height-1)){",
                "          if(Random.Range(0,2,true) == 0){",
                "                this.AddBlock(i,j);",
                "          }",
                "        }else{",
                "          this.AddPlayer(i,j,playerNumber);",
                "          playerNumber++;",
                "        }",
                "      }",
                "    }",
                "  }",
                "  ",
                "  //Update is called once per frame",
                "  Update() {",
                "  ",
                "  }",
                "",
                "  AddBlock(i,j){",
                "    var block = Instantiate(\"Block\");",
                "",
                "    var pos = this.transform.position;",
                "    var xx = pos.x - (this.width/2) + i + (block.transform.localScale.x/2);",
                "    var yy = pos.y + (block.transform.localScale.y/2) + (this.transform.localScale.y/2);",
                "    var zz =  pos.z - (this.height/2) + j + (block.transform.localScale.z/2);",
                "    block.transform.position = new Vector3(xx, yy, zz)",
                "  }",
                "",
                "  AddPlayer(i,j,pNum){",
                "    var block = Instantiate(\"Highlight\");",
                "",
                "    var pos = this.transform.position;",
                "    var xx = pos.x - (this.width/2) + i + (block.transform.localScale.x/2);",
                "    var yy = pos.y + (block.transform.localScale.y/2) + (this.transform.localScale.y/2);",
                "    var zz =  pos.z - (this.height/2) + j + (block.transform.localScale.z/2);",
                "    block.transform.position = new Vector3(xx, yy, zz)",
                "  }",
                "}",
                ""
            ]
