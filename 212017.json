                "Mouse.ts": "class Mouse extends MonoBehaviour {\n  @SerializeField\n  speed = 5;\n\n  anim: Animator;\n  rb: Rigidbody;\n  cam: Camera;\n  mouse: Mouse;\n\n  Start() {\n    this.anim = this.GetComponentInChildren(Animator);\n    this.cam = this.GetComponentInChildren(Camera);\n    this.rb = this.GetComponent(Rigidbody);\n    this.mouse = GameObject.FindObjectOfType(Mouse);\n  }\n\n\n  Update() {\n\n    let h = Input.GetAxis(\"Horizontal\");\n    let v = Input.GetAxis(\"Vertical\");\n\n    let input = Vector3.ClampMagnitude(new Vector3(h, 0, v), 1);\n    input.scaleInPlace(this.speed);\n\n    input.y = this.rb.velocity.y;\n\n    this.rb.velocity = input;\n\n    if (input.magnitude > this.stoppingDistance) {\n\n      this.anim.Play(\"Running\", 0.1);\n    } else {\n      this.rb.velocity = new Vector3();\n      this.anim.Play(\"Idle.001\", 0.1);\n    }\n  }\n}"
            },
            {
                "Doozy.ts": "//Doozy.ts\n\nclass Doozy extends MonoBehaviour {\n  @SerializeField\n  speed = 5;\n\n  @SerializeField\n  stoppingDistance = 1;\n\n  anim: Animator;\n  rb: Rigidbody;\n  mouse: Mouse;\n\n  Start() {\n    this.anim = this.GetComponentInChildren(Animator);\n    this.rb = this.GetComponent(Rigidbody);\n    this.mouse = GameObject.FindObjectOfType(Mouse);\n  }\n\n\n  Update() {\n    let direction = this.mouse.transform.position.negate(this.transform.position);\n    if (direction.magnitude > this.stoppingDistance) {\n      this.rb.velocity = direction.normalized.scale(this.speed);\n      this.anim.Play(\"Running\", 0.1);\n    } else {\n      this.rb.velocity = new Vector3();\n      this.anim.Play(\"Idle.001\", 0.1);\n    }    \n  }\n}"
            }
