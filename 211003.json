                "MultiMaterial": "@ExecuteInEditMode\nclass MultiMaterial extends MonoBehaviour {\n\n  @SerializeField\n  matNames : string[] = new Array()\n  \n  //Start is called before the first frame update\n  Start() {\n    var r = this.GetComponent(Renderer);\n    r.onChangedMesh.addOnce(e => {     \n\n      var meshes = r.GetMeshes();\n\n      for (var i = 0; i < meshes.length; i++)\n      {\n        if (i < this.matNames.length)\n        {\n          meshes[i].material = Material.Get(this.matNames[i]).shader;\n        }\n      }\n      \n    });\n\n  }\n}\n"
            },
            {
                "MainLight": "@ExecuteInEditMode\nclass MainLight extends MonoBehaviour {\n\n  @SerializeField\n  intensity = 1;\n\n  //Start is called before the first frame update\n  Start() {\n    \n  }\n  \n  //Update is called once per frame\n  Update() {\n    Lighting.mainLight.intensity = this.intensity;\n  }\n}\n"
            },
            {
                "SimpleRotateCam": "class SimpleRotateCam extends MonoBehaviour {\n\n  @SerializeField\n  rotateSpeed = 10;\n\n  minRot = 10;\n  maxRot = 90;\n\n  rotX = 0;\n  rotY = 0;\n\n  @SerializeField\n  zoomSpeed = 10;\n\n  minZoom = 1;\n  maxZoom = 10;\n\n\n  zoomTarget = 0;\n\n  onMouseMove: BABYLON.Observer<any>;\n  \n\n  //Start is called before the first frame update\n  Start() {\n    this.rotX = this.transform.parent.localEulerAngles.x;\n    this.rotY = this.transform.parent.localEulerAngles.y;\n\n    this.zoomTarget = this.transform.localPosition.x;\n  }\n\n  OnEnable ()\n  {\n    this.onMouseMove = Engine.scene.onPointerObservable.add(pointer => {\n      var evt = pointer.event as WheelEvent;\n      this.zoomTarget -= evt.deltaY * this.zoomSpeed;\n      this.zoomTarget = Mathf.Clamp(this.zoomTarget, this.minZoom, this.maxZoom);\n      this.transform.localPosition.x = this.zoomTarget;\n\n    }, BABYLON.PointerEventTypes.POINTERWHEEL, false);\n  }\n\n  OnDisable()\n  {\n    Engine.scene.onPointerObservable.remove(this.onMouseMove);\n  }\n\n  //Update is called once per frame\n  Update() {\n    if (Input.GetMouseButton(0)) {\n      Cursor.lockState = CursorLockMode.Locked;\n\n      let hor = Input.GetAxis(\"Mouse X\");\n      let ver = Input.GetAxis(\"Mouse Y\");\n      this.rotX += Time.deltaTime * ver * this.rotateSpeed;\n      this.rotX = Mathf.Clamp(this.rotX, 10, 90);\n      this.rotY += Time.deltaTime * hor * this.rotateSpeed;\n      this.transform.parent.localEulerAngles = new Vector3();\n      this.transform.parent.Rotate(0, this.rotY, 0, BABYLON.Space.WORLD);\n      this.transform.parent.Rotate(this.rotX, 0, 0, BABYLON.Space.LOCAL);\n\n    } else {\n          Cursor.lockState = CursorLockMode.None;\n\n    }\n\n    \n    \n  }\n}\n"
            }
